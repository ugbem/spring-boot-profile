pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    def mvnHome = tool 'Maven'
                    sh "${mvnHome}/bin/mvn clean install"
                }
            }
        }

        stage('Deploy') {
            steps {
                script {
                    sh "java -jar target/spring-demo-1.0-SNAPSHOT.jar &"

                }
            }
        }
    }
}

